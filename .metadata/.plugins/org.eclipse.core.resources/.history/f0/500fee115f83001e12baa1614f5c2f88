package avengers;

public class Array {

	public static void main(String[] args) {

		double nota[] = new double[2];

		nota[0] = 8.4;
		nota[1] = 4.5;

//	        System.out.println(Arrays.toString(nota));

		double media = 0;

		for (int i = 0; i < nota.length; i++) {
			media += nota[i]; // media = media + nota[i]
		}

		System.out.println("MÃ©dia do ALuno: " + media / nota.length);
	}
}
