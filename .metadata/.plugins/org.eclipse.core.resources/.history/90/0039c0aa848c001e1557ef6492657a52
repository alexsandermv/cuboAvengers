package exemplo;

public class ContaTeste {

	public static void main(String[] args) {

		Conta c1 = new Conta();
		ContaCorrente cc = new ContaCorrente();
		
		cc.abrirConta();
		System.out.println(cc.getSaldo());
		
		cc.depositar(1000);
		
		cc.consultaSaldo();
		
		cc.transferir(1500);
		
		cc.consultaSaldo();
		
		
	}

}
