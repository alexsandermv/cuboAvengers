package avengers;

public class OperadorLogico {

	public static void main(String[] args) {
		int x,y,z;
		x = 4;
		y = 7;
		z = 12;
		int idade = 17;
		
		boolean resultado;
		
		resultado = (x < y && y!= z) ? true:false; // && = condição E
		
		System.out.println("resultado: " + resultado);

	}

}
